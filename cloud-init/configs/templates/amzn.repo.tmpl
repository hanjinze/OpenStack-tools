[$name]
name=$name-$version-Base
mirrorlist=http://$mirror/$name-$version/mirror.list
mirror_expire=300
metadata_expire=300
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-amazon-beta
enabled=0

[$name-nosrc]
name=$name-$version-nosrc
mirrorlist=http://$mirror/$name-$version/nosrc-mirror.list
mirror_expire=300
metadata_expire=300
gpgcheck=0
enabled=0

[$name-debuginfo]
name=$name-$version-debuginfo
mirrorlist=http://$mirror/$name-$version/debuginfo-mirror.list
mirror_expire=300
metadata_expire=300
gpgcheck=1
enabled=0
